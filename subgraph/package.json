{
  "name": "@molecule/ipfnt-subgraph",
  "license": "UNLICENSED",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "create-ipnft-abi": "cat ../out/IPNFT.sol/IPNFT.json | jq .abi > ./abis/IPNFT.json",
    "create-simpleopensea-abi": "cat ../out/SimpleOpenSea.sol/SimpleOpenSea.json | jq .abi > ./abis/SimpleOpenSea.json",
    "prepare:local": "mustache config/local.js subgraph.template.yaml > subgraph.yaml",
    "prepare:goerli": "mustache config/goerli.json subgraph.template.yaml > subgraph.yaml",
    "prepare:mainnet": "mustache config/mainnet.json subgraph.template.yaml > subgraph.yaml",
    "deploy": "graph deploy --node https://api.thegraph.com/deploy/ moleculeprotocol/IP-NFT",
    "create-local": "graph create --node http://localhost:8020/ moleculeprotocol/IP-NFT",
    "remove-local": "graph remove --node http://localhost:8020/ moleculeprotocol/IP-NFT",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 moleculeprotocol/IP-NFT",
    "test": "graph test"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.35.0",
    "@graphprotocol/graph-ts": "0.28.1",
    "dotenv": "^16.0.3",
    "mustache": "^4.2.0"
  },
  "devDependencies": {
    "matchstick-as": "0.5.0"
  }
}
